<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_lista_celular"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListaCelular">

    <ListView
        android:id="@+id/lv_lista_celular"
        android:layout_width="263dp"
        android:layout_height="391dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.425"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.335" />

    <Button
        android:id="@+id/bt_agregar_celular"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="88dp"
        android:text="Agregar"
        app:layout_constraintEnd_toEndOf="@+id/lv_lista_celular"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/lv_lista_celular"
        app:layout_constraintTop_toBottomOf="@+id/lv_lista_celular" />

    <Button
        android:id="@+id/bt_actualizar_celulares"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:text="Actualizar"
        app:layout_constraintBottom_toBottomOf="@+id/bt_agregar_celular"
        app:layout_constraintStart_toEndOf="@+id/bt_agregar_celular"
        app:layout_constraintTop_toTopOf="@+id/bt_agregar_celular"
        app:layout_constraintVertical_bias="1.0" />
</androidx.constraintlayout.widget.ConstraintLayout>