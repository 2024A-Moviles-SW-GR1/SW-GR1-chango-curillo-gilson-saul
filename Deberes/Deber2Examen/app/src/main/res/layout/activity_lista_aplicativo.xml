<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_lista_aplicativo"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListaAplicativo">

    <ListView
        android:id="@+id/lv_lista_aplicativo"
        android:layout_width="270dp"
        android:layout_height="475dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.261" />

    <Button
        android:id="@+id/bt_agregar_aplicativo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="Agregar"
        app:layout_constraintEnd_toEndOf="@+id/lv_lista_aplicativo"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="@+id/lv_lista_aplicativo"
        app:layout_constraintTop_toBottomOf="@+id/lv_lista_aplicativo" />

    <Button
        android:id="@+id/bt_actualizar_lista_aplicativo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:text="Actualizar"
        app:layout_constraintBottom_toBottomOf="@+id/bt_agregar_aplicativo"
        app:layout_constraintStart_toEndOf="@+id/bt_agregar_aplicativo"
        app:layout_constraintTop_toTopOf="@+id/bt_agregar_aplicativo"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>